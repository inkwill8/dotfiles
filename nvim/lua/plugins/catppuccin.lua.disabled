return { 
      "catppuccin/nvim", 
      name = "catppuccin", 
      priority = 1000,
      config = function()
        require("catppuccin").setup()
        vim.cmd.colorscheme("catppuccin")
        vim.cmd("highlight CursorLine ctermbg=darkgrey guibg=#2a2a2a")
        vim.cmd("highlight CursorLineNr ctermfg=white guifg=white cterm=bold gui=bold")
      end
}
